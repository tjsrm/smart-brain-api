#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/d/WebZTM/PROJECTS/smartbrain/node_modules/.pnpm/nopt@1.0.10/node_modules/nopt/bin/nopt.js/node_modules:/mnt/d/WebZTM/PROJECTS/smartbrain/node_modules/.pnpm/nopt@1.0.10/node_modules/nopt/bin/node_modules:/mnt/d/WebZTM/PROJECTS/smartbrain/node_modules/.pnpm/nopt@1.0.10/node_modules/nopt/node_modules:/mnt/d/WebZTM/PROJECTS/smartbrain/node_modules/.pnpm/nopt@1.0.10/node_modules:/mnt/d/WebZTM/PROJECTS/smartbrain/node_modules/.pnpm/node_modules:/mnt/d/WebZTM/PROJECTS/smartbrain/node_modules:/mnt/d/WebZTM/PROJECTS/node_modules:/mnt/d/WebZTM/node_modules:/mnt/d/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../nopt@1.0.10/node_modules/nopt/bin/nopt.js" "$@"
else 
  exec node  "$basedir/../../../../../nopt@1.0.10/node_modules/nopt/bin/nopt.js" "$@"
fi
