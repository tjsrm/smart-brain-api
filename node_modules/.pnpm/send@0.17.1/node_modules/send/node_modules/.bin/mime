#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/d/WebZTM/PROJECTS/smartbrain/node_modules/.pnpm/mime@1.6.0/node_modules/mime/cli.js/node_modules:/mnt/d/WebZTM/PROJECTS/smartbrain/node_modules/.pnpm/mime@1.6.0/node_modules/mime/node_modules:/mnt/d/WebZTM/PROJECTS/smartbrain/node_modules/.pnpm/mime@1.6.0/node_modules:/mnt/d/WebZTM/PROJECTS/smartbrain/node_modules/.pnpm/node_modules:/mnt/d/WebZTM/PROJECTS/smartbrain/node_modules:/mnt/d/WebZTM/PROJECTS/node_modules:/mnt/d/WebZTM/node_modules:/mnt/d/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mime@1.6.0/node_modules/mime/cli.js" "$@"
else 
  exec node  "$basedir/../../../../../mime@1.6.0/node_modules/mime/cli.js" "$@"
fi
