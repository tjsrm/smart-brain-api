#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/d/WebZTM/PROJECTS/smartbrain/node_modules/.pnpm/atob@2.1.2/node_modules/atob/bin/atob.js/node_modules:/mnt/d/WebZTM/PROJECTS/smartbrain/node_modules/.pnpm/atob@2.1.2/node_modules/atob/bin/node_modules:/mnt/d/WebZTM/PROJECTS/smartbrain/node_modules/.pnpm/atob@2.1.2/node_modules/atob/node_modules:/mnt/d/WebZTM/PROJECTS/smartbrain/node_modules/.pnpm/atob@2.1.2/node_modules:/mnt/d/WebZTM/PROJECTS/smartbrain/node_modules/.pnpm/node_modules:/mnt/d/WebZTM/PROJECTS/smartbrain/node_modules:/mnt/d/WebZTM/PROJECTS/node_modules:/mnt/d/WebZTM/node_modules:/mnt/d/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../atob@2.1.2/node_modules/atob/bin/atob.js" "$@"
else 
  exec node  "$basedir/../../../../../atob@2.1.2/node_modules/atob/bin/atob.js" "$@"
fi
