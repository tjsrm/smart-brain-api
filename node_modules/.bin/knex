#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/d/WebZTM/PROJECTS/smartbrain/node_modules/.pnpm/knex@0.21.6_pg@8.4.0/node_modules/knex/bin/cli.js/node_modules:/mnt/d/WebZTM/PROJECTS/smartbrain/node_modules/.pnpm/knex@0.21.6_pg@8.4.0/node_modules/knex/bin/node_modules:/mnt/d/WebZTM/PROJECTS/smartbrain/node_modules/.pnpm/knex@0.21.6_pg@8.4.0/node_modules/knex/node_modules:/mnt/d/WebZTM/PROJECTS/smartbrain/node_modules/.pnpm/knex@0.21.6_pg@8.4.0/node_modules:/mnt/d/WebZTM/PROJECTS/smartbrain/node_modules/.pnpm/node_modules:/mnt/d/WebZTM/PROJECTS/smartbrain/node_modules:/mnt/d/WebZTM/PROJECTS/node_modules:/mnt/d/WebZTM/node_modules:/mnt/d/node_modules:/mnt/d/WebZTM/PROJECTS/smartbrain/node_modules/knex/bin/cli.js/node_modules:/mnt/d/WebZTM/PROJECTS/smartbrain/node_modules/knex/bin/node_modules:/mnt/d/WebZTM/PROJECTS/smartbrain/node_modules/knex/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../knex/bin/cli.js" "$@"
else 
  exec node  "$basedir/../knex/bin/cli.js" "$@"
fi
